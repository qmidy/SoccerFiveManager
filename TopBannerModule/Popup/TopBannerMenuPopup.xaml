<Popup x:Class="TopBannerModule.TopBannerMenuPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Name="TopBannerMenuPopupName" Height="200" Width="200">
    <UniformGrid Rows="3">
        <Button Width="50" Height="50" Click="MatchEngineClick">Moteur de match</Button>
        <Button Width="50" Height="50" Click="CalendarClick">Calendar</Button>
        <Button Width="50" Height="50" Click="ClubClick">Club</Button>
        <Button Width="50" Height="50" Click="TeamClick">Team</Button>
        <Button Width="50" Height="50" Click="TacticClick">Tactic</Button>
        <Button Width="50" Height="50" Click="MainMenuClick">Main Menu</Button>
    </UniformGrid>
</Popup>
